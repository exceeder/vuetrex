{
  "name": "@exceeder/vuetrex",
  "type": "module",
  "version": "0.5.0",
  "description": "WebGL animated diagram visualizations in 3D for Vue 3",
  "main": "dist/vuetrex.umd.cjs",
  "module": "dist/vuetrex.js",
  "unpkg": "dist/vuetrex.min.js",
  "typings": "./dist_types/vuetrex.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf ./dist && rm -rf ./dist_types",
    "build": "vue-tsc --declaration --emitDeclarationOnly && vite build",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "servedemo": "vue-cli-service serve dev/serve.ts --port 8080 --mode production",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:esm-project": "node test/esm-module/testNewProject.js",
    "tsc": "tsc",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "exports": {
    ".": {
      "import": "./dist/vuetrex.js",
      "require": "./dist/vuetrex.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exceeder/vuetrex.git"
  },
  "keywords": [
    "vue",
    "composition",
    "3d",
    "threejs",
    "diagram"
  ],
  "author": "Alex Pakka",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/exceeder/vuetrex/issues"
  },
  "homepage": "https://github.com/exceeder/vuetrex#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/three": "^0.143.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitest/ui": "latest",
    "@vue/test-utils": "^2.0.2",
    "happy-dom": "^6.0.4",
    "three": "^0.129.0",
    "typescript": "^4.6.4",
    "vite": "^3.0.0",
    "vitepress": "^1.0.0-alpha.4",
    "vitest": "^0.19.1",
    "vue": "^3.2.1",
    "vue-tsc": "^0.38.4"
  },
  "peerDependencies": {
    "three": "^0.129.0",
    "vue": "^3.2.1"
  },
  "engines": {
    "node": ">=14"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off"
    }
  }
}
