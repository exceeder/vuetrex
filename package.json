{
  "name": "@exceeder/vuetrex",
  "version": "0.1.0",
  "description": "Visualizing things in 3D using reactive Vue",
  "main": "dist/vuetrex.js",
  "scripts": {
    "docs": "vue-cli-service serve docs/main.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name Vue3dVit src/components/Vit.vue",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "dev": "webpack-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exceeder/vuetrex.git"
  },
  "keywords": [
    "vue",
    "composition",
    "3d",
    "threejs",
    "diargam"
  ],
  "author": "Alex Pakka",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/exceeder/vuetrex/issues"
  },
  "homepage": "https://github.com/exceeder/vuetrex#readme",
  "dependencies": {
    "@next-vue/runtime-test": "^3.0.0-alpha.8",
    "core-js": "^3.6.5",
    "three": "^0.120.1",
    "vue": "^3.0.2",
    "vue-class-component": "^8.0.0-beta.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@vue/cli-plugin-babel": "~4.5.4",
    "@vue/cli-plugin-eslint": "~4.5.4",
    "@vue/cli-plugin-typescript": "^4.5.8",
    "@vue/cli-service": "~4.5.8",
    "@vue/compiler-sfc": "^3.0.2",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.1.0",
    "typescript": "~4.0.5"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
