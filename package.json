{
  "name": "@exceeder/vuetrex",
  "version": "0.1.0",
  "description": "WebGL animated diagram visualizations in 3D for Vue 3",
  "main": "dist/vuetrex.umd.js",
  "browser": "dist/vuetrex.esm.js",
  "module": "dist/vuetrex.esm.js",
  "unpkg": "dist/vuetrex.min.js",
  "types": "vuetrex.d.ts",

  "files": [
    "dist/*",
    "vuetrex.d.ts",
    "src/**/*.vue"
  ],
  "sideEffects": false,

  "scripts": {
    "docs": "vue-cli-service serve docs/main.js",
    "serve": "vue-cli-service serve dev/serve.ts",
    "tsc": "tsc",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:umd": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format umd",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/exceeder/vuetrex.git"
  },
  "keywords": [
    "vue",
    "composition",
    "3d",
    "threejs",
    "diargam"
  ],
  "author": "Alex Pakka",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/exceeder/vuetrex/issues"
  },
  "homepage": "https://github.com/exceeder/vuetrex#readme",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@vue/cli-plugin-babel": "^4.5.7",
    "@vue/cli-plugin-typescript": "^4.5.7",
    "@vue/cli-service": "^4.5.7",
    "@vue/compiler-sfc": "^3.0.0",
    "cross-env": "^7.0.2",
    "minimist": "^1.2.5",
    "rollup": "^2.30.0",
    "rollup-plugin-postcss": "^3.1.8",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0-beta.11",
    "three": "^0.123.0",
    "typescript": "^3.8.3",
    "vue": "^3.0.3"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "engines": {
    "node": ">=10"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "off"
    }
  }
}
